# Ignore Node.js dependencies installed via npm or Yarn
/node_modules

# Ignore PnP (Plug 'n' Play) files and configuration (Yarn feature)
/.pnp
.pnp.js

# Ignore code coverage reports generated during testing
/coverage

# Ignore built output from the production build process
/build

# Ignore macOS-specific file metadata
.DS_Store

# Ignore local environment files for different environments
.env.local
.env.development.local
.env.test.local
.env.production.local

# Ignore logs generated by npm and Yarn during debugging
npm-debug.log*
yarn-debug.log*
yarn-error.log*
